        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:pq="http://schemas.pqlabs.com/itable/2010"
        Title="MouseSupport" Width="800" Height="600" SizeChanged="ITableWindow_SizeChanged">
    <Canvas>
        <Image Name="myImage" Source="penguins.jpg" Width="200" Height="150" Canvas.Left="83" Canvas.Top="144" />
